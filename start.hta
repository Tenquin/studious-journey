	<!-- General Function Library for utility functionality.  -->
	<script language="VBScript" src="vb/funcLib.vbs" type="text/VBScript"></script>
	<script type="text/javascript" src="js/funcLib.js"></script>

	<!-- Sub tasks or button tasks specific to this app -->
	<script language="VBScript" src="vb/tasks.vbs" type="text/VBScript"></script>

	<!-- init is for intializing global & enviromental variables as well as setting first load state of DOM -->
	<script language="VBScript" src="vb/init.vbs" type="text/VBScript"></script>

	<!-- Main Loop function switchboard which contols flow and state of application -->
	<script language="VBScript" src="vb/main.vbs" type="text/VBScript"></script>
	<!-- Kick off timer loop which runs in the main.vbs-->

</head>
<body>
		
	<!-- PageTile span -->
	<div id="divHeader" class="container">
		<span id="PageTitle" class="textvalignmiddle ">EFEC ECP 007 UPDATE</span>
		<span id="AppTitle">EFEC ECP 007</span>

		<!-- image files -->
		<img id="gear1" class="logo" src="bin/img/gear.png" />
		<img id="gear2" class="logo" src="bin/img/gear.png" />

	</div>

	<div id="myDiv" class="container">
		<!-- Buttons -->
		<div id="divButtons" class="container">
			<button id="btnRecover" type="button" class="btnX btn-warning" data-toggle="tooltip" title="This will start the recovery process. All data on this system will be wiped."
			 data-placement="right">
				<span class="glyphicon glyphicon-transfer well-sm"></span>Recover This System
			</button>
		</div>

		<!-- logSpan for text box update -->
		<span id="logSpan">
			<textarea name="logDump" id="logtext" cols="120" rows="4" class="log"></textarea>
		</span>
		
		<!-- ProcessList span. Main message text -->
		<div class="container">
			<span id="ProcessList">Please wait. Checking System Status...</span>
		</div>

		<!-- Task List -->
		<span ID="TMSpan" class="TaskSpan">Upgrade Tasks
			<table id = "TMTable" class="TaskTable">
				<tbody>
				<tr class="TableRow1">
					<th style="width:0%;">Status</th>
					<th>TASK</th>
				</tr>
				<tr>
					<td ><img id="taskUTCTimeZone" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskUTCTime">Setting Time Zone to UTC</span></td>
                </tr>
                <tr>
					<td ><img id="taskOSForensicsRegistration" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskOSForensicsReg">Registering OSForensics</span></td>
                </tr>
                <tr>
					<td ><img id="taskSetComputerName" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskSetCompName">Setting Computer Name</span></td>
                </tr>
                <tr>
					<td ><img id="taskSetBackgroundImage" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskSetBGRNDIMG">Setting Background Image</span></td>
                </tr>
                <tr>
					<td ><img id="taskActivateWindows" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskActWindows">Activating Windows</span></td>
                </tr>
                <tr>
					<td ><img id="taskActivateOffice" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskActOffice">Activating Office</span></td>
                </tr>
                <tr>
					<td ><img id="taskImportRegistryKeys" src="bin/img/start.png" class="lineImage"></td>
					<td><span id="taskImportRegKeys">Importing Registry Keys</span></td>
				</tr>
				</tbody>
			</table>
		</span>

		<!-- <div id="divPowerPanel" class="container">
			<button id="btnReboot" type="button" class="btn2 btn-primary" value="Reboot" onclick="Reboot()" data-toggle="tooltip"
			 title="Reboots this System" data-placement="left"><span class="glyphicon glyphicon-repeat well-sm"></span>Reboot</button>
			<button id="btnEject" type="button" onclick="tEject()" class="btn3 btn-primary" value="Eject CD" data-toggle="tooltip"
			 title="Eject CD/DVD/BD drive." data-placement="right"><span class="glyphicon glyphicon-eject well-sm"></span>Eject Disk</button>
		</div> -->
